 //"Resort_Bar": "{{i18n:"Resort_Bar_{{Random:{{Range: 0, 5}}}}}}",
 "Resort_Bar_0": "Ah I do love a good pina colada!",
 "Resort_Bar_1": "Drunk? Possibly!",
 "Resort_Bar_2": "I haven't been able to let loose in so long, it's quite nice.",
 "Resort_Bar_3": "Hmmm... warm sand between your toes and a drink in hand, it's quite nice.",
 "Resort_Bar_4": "I couldn't possibly be *hic* ...#$b#Okay maybe I'm a little drunk.",
 "Resort_Bar_5": "I'm not as think as you drunk I am...",

 //"Resort_Chair": "{{i18n:"Resort_Chair_{{Random:{{Range: 0, 5}}}}}}",
